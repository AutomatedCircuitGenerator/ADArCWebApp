@inherits RazorComponent

@code {
    
    [Parameter] public bool IsInPalette { get; set; } = false;
    private double _temperature = 1;

    [Parameter]
    [ValueSetting("C", 1, -55, 125)]
    public double temperature
    {
        get => _temperature;
        set
        {
            var hasChanged = value != _temperature;
            _temperature = value;
            if (Controller is not null && hasChanged)
            {
                InvokeAsync(async () => { await Controller.InvokeVoidAsync("setTemperature", _temperature); });
            }
        }
    }

    public static ElementPin[] pinInfo
    {
        get
        {
            return new ElementPin[]
            {
                new ElementPin(name: "gnd", x: 44, y: 110, signals: new PinSignalInfo[] { }, number: 1),
                new ElementPin(name: "5V", x: 54, y: 110, signals: new PinSignalInfo[] { PinSignalInfo.VCC() }, number: 2),
                new ElementPin(name: "DQ", x: 63, y: 110, signals: new PinSignalInfo[] { PinSignalInfo.GND() }, number: 3)
            };
        }
    }

}

