@using System.Text
@inherits RazorComponent

@code {

    /// <summary>
    /// RazorKY012.razor
    /// Defines the physical pins of the KY-012 Active Piezo-Buzzer:
    /// - digital_out: Signal pin - when HIGH (≥3.3V), generates 2.5kHz tone - Just using ON/OFF
    /// - +V: Not connected - this pin has no function
    /// - gnd: Ground connection
    /// </summary>
    public static ElementPin[] pinInfo
    {
        get
        {
            return new ElementPin[]
            {
                // Signal pin - controls buzzer state (ON/OFF)
                new ElementPin(
                    name: "digital_out",
                    x: 60,
                    y: 107,
                    signals: new PinSignalInfo[] { },
                    number: 1
                ),
                // Ground pin - completes the circuit
                new ElementPin(
                    name: "gnd",
                    x: 42,
                    y: 107,
                    signals: new PinSignalInfo[] { PinSignalInfo.GND() },
                    number: 3)
            };
        }
    }

}

