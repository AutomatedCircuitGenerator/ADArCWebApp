@inherits RazorComponent

@code {
    private string _pushed = "Released";
    
    [Parameter]
    [DropdownSetting(["Released", "Pushed"])]
    public string pushed
    {
        get => _pushed;
        set
        {
            _pushed = value;
            if (Controller is not null)
            {
                InvokeAsync(async () => { await Controller.InvokeVoidAsync("setPushed", _pushed == "Pushed"); });
            }
        }
    }
    
    public static ElementPin[] pinInfo
    {
        get
        {
            return new ElementPin[]
            {
                new ElementPin(name: "digital_out", x: 102, y: 158, signals: new PinSignalInfo[] { }, number: 1), //gnd
                new ElementPin(name: "gnd", x: 80, y: 158, signals: new PinSignalInfo[] { }, number: 2), //dgtl 
            };
        }
    }
}

