@* @inherits RazorComponent *@

@code {

    public static ElementPin[] pinInfo
    {
        get
        {
            return new ElementPin[]
            {
                new ElementPin(name: "GND", x: 20, y: 100, signals: new PinSignalInfo[] { }, number: 1),
                new ElementPin(name: "VCC", x: 30, y: 100, signals: new PinSignalInfo[] { }, number: 4),
                new ElementPin(name: "SCK", x: 40, y: 100, signals: new PinSignalInfo[] { }, number: 5),
                new ElementPin(name: "CS", x: 50, y: 100, signals: new PinSignalInfo[] { }, number: 6), //chip select. pull low to enable serial interface
                new ElementPin(name: "SO", x: 60, y: 100, signals: new PinSignalInfo[] { }, number: 7),
            };
        }
    }

    private double _temperature;
    [Parameter]
    [ValueSetting("°C", 20, 0, 1024)]// 1023.75*
    public double temperature
    {
        get => _temperature;
        set
        {
            Console.WriteLine("hi");
            _temperature = value;
        }
    }

}

