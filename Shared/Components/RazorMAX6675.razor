@inherits RazorComponent

@code {

    public static ElementPin[] pinInfo
    {
        get
        {
            return new ElementPin[]
            {
                new ElementPin(name: "VCC", x: 44, y: 181, signals: new PinSignalInfo[] { }, number: 1),
                new ElementPin(name: "GND", x: 37, y: 181, signals: new PinSignalInfo[] { }, number: 4),
                new ElementPin(name: "SO", x: 67, y: 181, signals: new PinSignalInfo[] { }, number: 5),
                new ElementPin(name: "CS", x: 60, y: 181, signals: new PinSignalInfo[] { }, number: 6), //chip select. pull low to enable serial interface
                new ElementPin(name: "SCK", x: 52, y: 181, signals: new PinSignalInfo[] { }, number: 7),
            };
        }
    }

    private double _temperature = 20;

    [Parameter]
    [ValueSetting("°C", 20, 0, 1024)] // 1023.75*
    public double temperature
    {
        get => _temperature;
        set
        {
            _temperature = value;
            if (Controller is not null)
            {
                InvokeAsync(async () => { await Controller.InvokeVoidAsync("setTemperature", _temperature); });
            }
        }
    }
}

