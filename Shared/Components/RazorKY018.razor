@inherits RazorComponent

@code {

    public static ElementPin[] pinInfo
    {
        get
        {
            return new ElementPin[]
            {
                new ElementPin(name: "analog_out", x: 43, y: 130, signals: new PinSignalInfo[] { }, number: 1),
                new ElementPin(name: "5V", x: 53, y: 130, signals: new PinSignalInfo[] { }, number: 2),
                new ElementPin(name: "gnd", x: 63, y: 130, signals: new PinSignalInfo[] { }, number: 3),
            };
        }
    }

    private double _lux;

    [Parameter]
    [ValueSettingDouble("L", 100, 0.1, 100000)]
    public double lux
    {
        get => _lux;
        set
        {
            var hasChanged = value != _lux;
            _lux = value;
            if (Controller is not null && hasChanged)
            {
                InvokeAsync(async () => { await Controller.InvokeVoidAsync("setLux", _lux); });
            }
        }
    }

}

